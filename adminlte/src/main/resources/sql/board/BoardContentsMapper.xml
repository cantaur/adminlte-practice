<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cantaur.adminlte.mapper.board.BoardContentsMapper">

    <select id="countTotalBbsContents" resultType="int" parameterType="BoardContentsReq">
        SELECT
            count(1)
        FROM
            board
    </select>


    <select id="selectBbsContentsList" resultType="BbsContentsDto">
        SELECT
            *
        FROM
            tb_bbs_contents
    </select>

</mapper>